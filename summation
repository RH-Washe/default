#include <iostream>
using namespace std;

int main() {
    int N;
    cin >> N;

    long long sum = 0; // long long to handle large sums
    int num;

    for (int i = 0; i < N; i++) {
        cin >> num;
        sum += num;
    }

    if (sum < 0) sum = -sum; // take absolute value

    cout << sum << endl;
    return 0;
}
